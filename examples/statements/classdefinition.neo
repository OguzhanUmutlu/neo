class MyClass {
    constructor(a) {
        this.a = a
        this.b = 20
    }

    increaseA() {
        this.a++
    }
}

class MyExtendedClass extends MyClass {
    constructor(a, d) {
        super(a)
        this.d = 20
    }

    increaseB() {
        super.increaseA()
        this.b++
    }
}